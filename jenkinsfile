pipeline{
    agent any
    stage('Deploy') {
             sh ''' #! /bin/bash

             aws deploy create-deployment --application-name shubhamtf --deployment-group-name chatapp-dg-tf --deployment-config-name CodeDeployDefault.AllAtOnce --github-location repository=https://github.com/1996Shubh/Chatapp_project,commitId=e52ed132924cd26631640cf04daca686fefdbea2

             '''
    }


}
